[twitterapi]
consumer_key: xxxx
consumer_secret: xxxx
access_token_key: xxxx
access_token_secret: xxxx
status_file: /var/run/tweetsniff.status

[highlight]
color: red
regex: foo
	security

[search]
color: blue
keywords: drone
	brucon
	malware

[elasticsearch]
server: http://127.0.0.1:9200
index: twitter-%Y.%m.%d

[cef]
server: 192.168.0.1
port: 514

[urls]
ua: "Mozilla/5.0 (Windows NT 6.3; rv:36.0) Gecko/20100101 Firefox/36.0"
timeout: 30
urls_process_urls: true
